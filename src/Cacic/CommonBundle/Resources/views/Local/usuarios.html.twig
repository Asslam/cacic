<h5>{{ "Usuários Associados"|trans }}</h5>
	
<table class="table table-striped table-bordered">
	<thead>
		<tr>
		    <th style="text-align: center"></th>
		    <th>{{ "Login"|trans }}</th>
		    <th>{{ "Nome Completo"|trans }}</th>
		    <th>{{ "Nível de acesso"|trans }}</th>
		    {#<th>{{ "Tipo de acesso"|trans }}</th>#}
		    <th>{{ "E-mail"|trans }}</th>
			<th></th>
		</tr>
	</thead>
	
  	<tbody>
	
    {% for usuario in usuarios %}
    
    <tr id="item_{{ usuario[0]['idUsuario'] }}">
	    <td style="text-align: center">{{ loop.index }}</td>
	    <td id="item_desc_{{ usuario[0]['idUsuario'] }}">{{ usuario[0]['nmUsuarioAcesso'] }}</td>
	    <td>{{ usuario[0]['nmUsuarioCompleto'] }}</td>
	    <td>{{ usuario['teGrupoUsuarios'] }}</td>
	    {#<td>{% if idLocal == usuario['idLocal'] %} Primário {%else%} Secundário {% endif %}  </td>#}
	    <td>{{ usuario[0]['teEmailsContato'] }}</td>
        <td>
            <a href="{{ path('cacic_usuario_editar', {'idUsuario': usuario[0]['idUsuario']}) }}" class="btn btn-small" title="{{ "Editar Dados do Usuário"|trans }}" target="_blank">
                <i class="btn-icon-only icon-edit icon-large"></i>
            </a>
        </td>
    </tr>
    
    {% else %}
    <tr>
		<td colspan="6" style="text-align: center;"><b>{{ "NENHUM REGISTRO ENCONTRADO!"|trans }}</b></td>
	</tr>
	{% endfor %}
    
  </tbody>
</table>