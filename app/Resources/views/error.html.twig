<!DOCTYPE html>
<html lang="pt-BR"><head>
    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <meta charset="utf-8">
    <title>{% block title %}{{ 'Cacic - Configurador Automático e Coletor de Informações Computacionais'|trans }}{% endblock %}</title>

    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <meta name="apple-mobile-web-app-capable" content="yes">

    {% block javascripts %}
        {% javascripts output='assets/error.js' combine=true
        '@jquery_js'
        '@bootstrap_js'
        %}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}
    {% endblock %}

    {% block stylesheets %}
        {% stylesheets output='assets/error.css' combine=true filter='cssrewrite'
        '@bootstrap_less'
        'bundles/caciccommon/css/*'
        'bundles/caciccommon/Font-Awesome-3.0.2/css/font-awesome.min.css'
        %}
        <link href="{{ asset_url }}" type="text/css" rel="stylesheet" media="screen" />
        {% endstylesheets %}

        <link href="{{ asset('bundles/caciccommon/libs/googleapis.fonts.css') }}" rel="stylesheet">

    {% endblock %}

</head>

<body>

<div class="navbar navbar-fixed-top">

    <div class="navbar-inner">

        <div class="container">

            <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </a>

            <a class="brand" href="{{ path('cacic_common_homepage') }}">
                CACIC v.{{ cacic_version }}{{' - '}}{{ url }}
            </a>

            <div class="nav-collapse">

            </div><!--/.nav-collapse -->

        </div> <!-- /container -->

    </div> <!-- /navbar-inner -->

</div> <!-- /navbar -->

<div class="span12">
    <div class="content clearfix">

        {% block body %}
        {% endblock %}

    </div> <!-- /content -->

</div> <!-- /span12 -->

</body>
</html>
