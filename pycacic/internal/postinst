chmod +x /usr/share/pycacic/*.py -R
chmod +w /usr/share/pycacic/logs -R
if [ "$$DISPLAY" = "" ]; then
    (nohup python /usr/share/pycacic/mapacacic.py > /dev/null 2>&1)
else
    (nohup python /usr/share/pycacic/gui_mapacacic.py > /dev/null 2>&1)&
fi
if [ "$$DISPLAY" != "" ]; then
    (nohup python /usr/share/pycacic/gui_pycacic.py > /dev/null 2>&1)&
fi
(nohup /etc/init.d/cacic start > /dev/null 2>&1)&
